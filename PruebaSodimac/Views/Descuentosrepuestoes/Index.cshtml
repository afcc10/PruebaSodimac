@model IEnumerable<PruebaSodimac.Models.Descuentosrepuesto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fechavigencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Valordescuento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Porcentajedescuentos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodrepuestoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fechavigencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Valordescuento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Porcentajedescuentos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodrepuestoNavigation.IdRepuesto)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Rowiddescuentos">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Rowiddescuentos">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Rowiddescuentos">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
